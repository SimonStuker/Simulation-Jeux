  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run PYTHON 50missions.py",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/50mission.py",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Run RUST jeu_50missions (Release)",
      "type": "lldb",
      "request": "launch",
      "cargo": {
        "args": [
          "build",
          "--package", "jeu_50missions",
          "--release"
        ],
        "filter": {
          "name": "50missions",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Debug RUST jeu_50missions",
      "type": "lldb",
      "request": "launch",
      "cargo": {
        "args": [
          "build",
          "--package", "jeu_50missions"
        ],
        "filter": {
          "name": "50missions",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}